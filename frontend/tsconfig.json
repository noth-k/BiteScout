{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "jsx": "react",
    "module": "esnext",
    "allowJs": true,
    "checkJs": false,
    "strict": true,
    "baseUrl":".",
    "allowSyntheticDefaultImports": true,
    "paths": {
      "@/*": ["src/*"],
      "@components/*": ["src/components/*"],
      "@assets/*": ["assets/*"]
    },
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
, "src/context/AuthContext.js", "src/hooks/useAuthContext.js", "src/hooks/useSignUp.js"  ]
}
