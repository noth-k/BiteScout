{
  "name": "bitescout",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "test": "node --no-warnings --experimental-vm-modules $( [ -f ./node_modules/.bin/jest ] && echo ./node_modules/.bin/jest || which jest )"
  },
  "jest": {
    "preset": "jest-expo",
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?react-native|@react-native(-community)?|expo(nent)?|@expo(nent)?/.*|@expo-google-fonts/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|react-native-svg|ky)"
    ],
    "transform": {
      "^.+\\.js$": "babel-jest",
      "^.+\\.tsx?$": "ts-jest"
    },
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js"
    },
    "setupFiles": ["<rootDir>/__tests__/jestSetupFile.js"]
  },
  "dependencies": {
    "@expo/vector-icons": "^14.0.0",
    "@nlpjs/lang-en-min": "^4.26.1",
    "@nlpjs/nlp": "^4.27.0",
    "@phosphor-icons/react": "^2.1.5",
    "@react-icons/all-files": "^4.1.0",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-native-community/geolocation": "^3.2.1",
    "@react-native-community/slider": "^4.5.2",
    "@react-native-picker/picker": "^2.7.7",
    "@react-navigation/native": "^6.0.2",
    "@testing-library/react-native": "^12.5.1",
    "axios": "^1.7.2",
    "compromise": "^14.13.0",
    "expo": "~51.0.8",
    "expo-font": "~12.0.5",
    "expo-linking": "~6.3.1",
    "expo-location": "^17.0.1",
    "expo-router": "~3.5.14",
    "expo-splash-screen": "~0.27.4",
    "expo-status-bar": "~1.12.1",
    "expo-system-ui": "~3.0.4",
    "expo-updates": "~0.25.16",
    "expo-web-browser": "~13.0.3",
    "ky": "^1.2.4",
    "natural": "^7.1.0",
    "porter-stemmer": "^0.9.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "^5.2.1",
    "react-native": "^0.74.1",
    "react-native-animated-emoji": "^0.1.4",
    "react-native-dropdown-picker": "^5.4.6",
    "react-native-geolocation-service": "^5.3.1",
    "react-native-google-places-autocomplete": "^2.5.6",
    "react-native-reanimated": "~3.10.1",
    "react-native-safe-area-context": "4.10.1",
    "react-native-screens": "3.31.1",
    "react-native-share": "^10.2.1",
    "react-native-web": "~0.19.10",
    "spacy-nlp": "^1.0.11",
    "wink-eng-lite-web-model": "^1.8.0",
    "wink-lemmatizer": "^3.0.4",
    "wink-nlp": "^2.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@expo/ngrok": "4.1.0",
    "@types/react": "~18.2.45",
    "@types/react-native": "^0.73.0",
    "jest": "^29.7.0",
    "jest-expo": "~51.0.2",
    "react-test-renderer": "^18.2.0",
    "ts-jest": "^29.2.3",
    "typescript": "~5.3.3"
  },
  "private": true
}
